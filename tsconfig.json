{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"declaration": true,
		"outDir": "./lib",
		"strict": true,
		"downlevelIteration": true
	},
	"tsc-alias": {
		"resolveFullPaths": true,
		"fileExtensions": "{js,d.ts}"
	},
	"include": [
		"src"
	],
	"exclude": [
		"**/node_modules/**",
		"src/test.ts"
	]
}